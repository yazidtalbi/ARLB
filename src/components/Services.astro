---
import HeaderLink from './HeaderLink.astro';
import { SITE_TITLE } from '../consts';
---

 <div class="home-2-process-section">
        <div class="green-gradient-div home-2-process-green-gradient">
          <img
            src="../../assets.website-files.com/6259e59720a6347d57a4fe10/62c2f60ab44ea93dd695bd1c_Ellipse%209%20(1).png"
            loading="lazy"
            sizes="(max-width: 991px) 100vw, 774px"
            srcset="
              https://assets.website-files.com/6259e59720a6347d57a4fe10/62c2f60ab44ea93dd695bd1c_Ellipse%209%20(1)-p-500.png  500w,
              https://assets.website-files.com/6259e59720a6347d57a4fe10/62c2f60ab44ea93dd695bd1c_Ellipse%209%20(1).png       1193w
            "
            alt=""
            class="green-gradient-image"
          />
        </div>
        <div class="home-2-process-gradient-div">
          <div class="green-gradient-div home-2-process-green-div">
            <img
              src="../../assets.website-files.com/6259e59720a6347d57a4fe10/62c2ebb10d13eebf4bf1dd46_Ellipse%204%20(1).png"
              loading="lazy"
              sizes="(max-width: 767px) 100vw, 489px"

---
const posts = [
  {
    type: "Design",
    image: 'https://images.blush.design/74760cde0d8fffeb1f396ad8fe1ad6ac?w=500&auto=compress&cs=srgb',
    title:
      "The Worst Advice We've Ever Heard About Web Design",
  },
  {
    type: "Marketing",
    image: 'https://images.blush.design/afff201a0486036c4b8efaac686a6e6e?w=500&auto=compress&cs=srgb',
    title:
      "7 Things About Web Design Your Boss Wants To Know",
  },
  {
    type: "Développement",
    image: 'data:image/svg+xml;base64,',
    title:
      "The History Of Web Design in Brief",
  },

];




---
<style>
        a:link {
    color: #000; /* Set the desired text color for unvisited links */
    text-decoration: none; /* Remove the underline for unvisited links */
}

/* Styling visited links */
a:visited {
    color: #000; /* Set the desired text color for visited links */
    text-decoration: none; /* Remove the underline for visited links */
}
</style>

<div class='overview-wrapper'>
  <div class='overview-top-wrapper'>
    <div class='home-2-recent-blog-upper-wrap'>
      <h2
        class='h2-heading-56px'
        style='max-width: 810px; margin-bottom: 40px;'
      >
        Découvrez nos meilleures <span class='boska-variable-text-italic'
          >astuces techniques</span
        > ainsi que les <span class='boska-variable-text-italic'>tendances</span
        > du monde numérique.
      </h2><a href='/blog' class='link-block-wrapper no-margin w-inline-block'
        ><div
          class='recent-blog-black-box'
          style='transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg); transform-style: preserve-3d; background-color: rgb(0, 0, 0);'
        >
          <img
            src='https://assets.website-files.com/6259e59720a6347d57a4fe10/6259e59720a6349779a4fe5d_ArrowUpRight.svg'
            loading='lazy'
            alt=''
            class='arrow-image-link-block'
            style='filter: invert(0%);'
          />
        </div><div class='recent-blog-bold-text-16px'>Derniers articles</div></a
      >
    </div>
  </div>

  <div class='w-layout-grid overview-grid'>


  {posts.map((item) => (
    

    <div
      class='about-3-press-hero-wrapper'
      style='opacity: 1; transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg); transform-style: preserve-3d;    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;'
    
    >
      <img
        src={item.image}
        loading='lazy'
      
        alt=''
        class='about-3-press-item-icon' style="  max-height:220px"
      />

      <a
        href='/blog'
        data-w-id='5068ba5a-f1d7-d9b7-585a-241623b17d6c'
        class='overview-grid-wrap w-inline-block'
        ><div class='recent-blog-text-upper-wrapper'>
          <div 
            style='
                                  background-color: hsla(
                                    56.25000000000001,
                                    100%,
                                    65.49%,
                                    1
                                  );
                                '
            class='recent-blog-category-text-span type'
          >
            {item.type}
          </div><div class='recent-blog-reading-time-div'>
            <div class='reading-time-text-14px-bold'>· 8 min read</div>
          </div>
        </div>
        
        <div class="home-2-blog-item-heading" style="  text-align: left;    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2; /* Limit the number of lines to 2 */
    -webkit-box-orient: vertical;">   {item.title}</div>
        
     
      </a>
    </div>
  ))}
 





    
  </div>
</div>

<script>
// Assuming item.type contains 'design', 'dev', or 'marketing'

const elements = document.querySelectorAll('.type');

elements.forEach((element: HTMLElement) => {
    const type = element.textContent || ''; // Get the text content of the element

    switch (type.trim().toLowerCase()) {
        case 'design':
            element.style.backgroundColor = 'hsla(136.70103092783503, 100.00%, 80.98%, 1.00)'; // Set custom color for design type
            break;
        case 'dév':
            element.style.backgroundColor = 'hsla(56.25000000000001, 100.00%, 65.49%, 1.00)'; // Set custom color for development type
            break;
        case 'marketing':
            element.style.backgroundColor = 'hsla(274.2857142857143, 100.00%, 82.16%, 1.00)'; // Set custom color for marketing type
            break;
        default:
            // Handle unknown type
            break;
    }
});
 
</script>
